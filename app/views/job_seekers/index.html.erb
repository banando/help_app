<p id="notice"><%= notice %></p>

<h1>Job Seekers</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Password</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>State</th>
      <th>City</th>
      <th>Bio</th>
      <th>Portfolio</th>
      <th>Social media</th>
      <th>Rating</th>
      <th>Comments</th>
      <th>Interests</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_seekers.each do |job_seeker| %>
      <tr>
        <td><%= job_seeker.username %></td>
        <td><%= job_seeker.password %></td>
        <td><%= job_seeker.first_name %></td>
        <td><%= job_seeker.last_name %></td>
        <td><%= job_seeker.email %></td>
        <td><%= job_seeker.state %></td>
        <td><%= job_seeker.city %></td>
        <td><%= job_seeker.bio %></td>
        <td><%= job_seeker.portfolio %></td>
        <td><%= job_seeker.social_media %></td>
        <td><%= job_seeker.rating %></td>
        <td><%= job_seeker.comments %></td>
        <td><%= job_seeker.interests %></td>
        <td><%= link_to 'Show', job_seeker %></td>
        <td><%= link_to 'Edit', edit_job_seeker_path(job_seeker) %></td>
        <td><%= link_to 'Destroy', job_seeker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job Seeker', new_job_seeker_path %>
