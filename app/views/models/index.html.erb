<p id="notice"><%= notice %></p>

<h1>Models</h1>

<table>
  <thead>
    <tr>
      <th>Employer</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Location</th>
      <th>Bio</th>
      <th>Company name</th>
      <th>Industy</th>
      <th>Company website</th>
      <th>City</th>
      <th>Social media links</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @models.each do |model| %>
      <tr>
        <td><%= model.employer %></td>
        <td><%= model.first_name %></td>
        <td><%= model.last_name %></td>
        <td><%= model.email %></td>
        <td><%= model.location %></td>
        <td><%= model.bio %></td>
        <td><%= model.company_name %></td>
        <td><%= model.industy %></td>
        <td><%= model.company_website %></td>
        <td><%= model.city %></td>
        <td><%= model.social_media_links %></td>
        <td><%= link_to 'Show', model %></td>
        <td><%= link_to 'Edit', edit_model_path(model) %></td>
        <td><%= link_to 'Destroy', model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Model', new_model_path %>
